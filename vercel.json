{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/categories/all",
      "dest": "/api/categories/all.js"
    },
    {
      "src": "/api/categories/:id",
      "dest": "/api/categories/[id].js"
    }
  ]
}
